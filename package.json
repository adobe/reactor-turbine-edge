    "test": "karma start",
    "test:watch": "npm test -- --no-single-run --no-coverage",
    "lint": "eslint 'src/**/*.js'"
  },
  "author": {
    "name": "Adobe Systems",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
